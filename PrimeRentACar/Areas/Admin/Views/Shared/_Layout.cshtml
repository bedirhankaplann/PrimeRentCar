                            </a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link" href="~/Admin/Admin/CreateCar">
                                <i class="ni ni-ambulance text-primary"></i>
                                <span class="nav-link-text">Araç Ekle</span>
                            </a>
                        </li>
                     
                    </ul>
                    <!-- Divider -->
                   
                </div>
            </div>
        </div>
    </nav>
    <div class="main-content" id="panel">
        @Html.Partial("NavbarPartial")
        <br>
        @Html.Partial("HeaderPartial")
        @RenderBody()
    </div>
        <!-- Argon Scripts -->
        <!-- Core -->
        <script src="/AdminTemplate/assets/vendor/jquery/dist/jquery.min.js"></script>
        <script src="/AdminTemplate/assets/vendor/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
        <script src="/AdminTemplate/assets/vendor/js-cookie/js.cookie.js"></script>
        <script src="/AdminTemplate/assets/vendor/jquery.scrollbar/jquery.scrollbar.min.js"></script>
        <script src="/AdminTemplate/assets/vendor/jquery-scroll-lock/dist/jquery-scrollLock.min.js"></script>
        <!-- Optional JS -->
        <script src="/AdminTemplate/assets/vendor/chart.js/dist/Chart.min.js"></script>
        <script src="/AdminTemplate/assets/vendor/chart.js/dist/Chart.extension.js"></script>
